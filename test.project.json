{
    "name": "watchdog-test",
    "tree": {
      "$className": "DataModel",

      "Players": {
        "$className": "Players",
        "$properties": {
          "CharacterAutoLoads": false
        }
      },

      "ReplicatedStorage": {
        "$className": "ReplicatedStorage",
        "Packages": {
            "$className": "Folder",
            "Watchdog": {
                "$path": "lib"
            }
        },

        "DevPackages": {
          "$path": "DevPackages"
        }
      },

      "TestService": {
        "$className": "TestService",
        "$properties": {
          "ExecuteWithStudioRun": true
        },

        "Test": {
          "$path": "test/runTests.server.luau"
        }
      }
    }
  }